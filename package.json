{
    "name": "shop",
    "version": "1.0.0",
    "description": "shop with angular",
    "main": "index.js",
    "engines": {
        "node": "14.15.1",
        "npm": "6.14.8"
    },
    "scripts": {
        "start": "node index.js",
        "server": "nodemon index.js",
        "client-install": "npm install --prefix client",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
    },
    "keywords": [
        "express",
        "angular"
    ],
    "author": "anderboom",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^6.3.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongoose": "^5.11.15",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "nodemon": "^2.0.14",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {}
}
